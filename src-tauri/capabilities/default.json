{
  "identifier": "default",
  "description": "Default capabilities for Lector",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "sql:default",
    "sql:allow-execute",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://**" }, { "url": "http://**" }]
    }
  ]
}
